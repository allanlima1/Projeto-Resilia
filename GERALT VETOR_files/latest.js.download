                    'ext': {
                        'gdpr': (gdprConsent.gdprApplies === true) ? 1 : 0
                    }
                };
                
                if (gdprConsent.consentString) {
                    // GDPR Stuff
                    request.user = {
                        'ext': {
                            'consent': gdprConsent.consentString
                        }
                    };
                }
