        }
        if (uspConsent.cmpPresent && uspConsent.usPrivacy) {
            query.push(['ccpa', uspConsent.usPrivacy])
        }
        if (query.length > 0) {
            url += '?' + query.map(function(q) { q.join('='); }).join('&');
        }
        userSyncs.add({
            type: 'iframe',
            url: url,
