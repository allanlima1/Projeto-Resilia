                                    } else {
                                        ad.adcode = adcode;
                                    }

                                    bidData.logBidResponse(ad);
                                });
        					});
                        }
                        
            			//log request as received regardless
                        bidData.incrementBidResponseCount(request_cnt);

                        // Sync User
                        addUserSyncs(request, resp);
                            
                    } catch (e) {
                        e.bidder = bidder;
