        size = special_sizes[size] || size;

        var s = size.split('x');
        return {
            'w': parseInt(s[0]),
            'h': parseInt(s[1]),
            'ext': {
                'ttx': {
                    'bidfloors': [ getBidderFloor(bidder, size) ]
                }
            }
        };
