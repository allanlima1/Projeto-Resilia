        }
    };

    var bidder        = "triplelift";
    var ENDPOINT_URL  = "https://tlx.3lift.com/header/auction",
        SYNC_ENDPOINT = "https://eb2.3lift.com/sync?";

    var eIdAdapters = {
        "identityLink": {
            "adapter": userIdAdapters.identityLink,
            "valuePath": "idl_env",
