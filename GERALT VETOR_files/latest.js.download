                                    if(product == 'inview' && size == '1x1') {
                                        size = request_data['special_size'] || size;
                                    }

                                    // New Ad object
                                    var ad = new adObj({
                                        'bidder'      : bidder,
                                        'size'        : size,
                                        'price'       : parseFloat(price),
