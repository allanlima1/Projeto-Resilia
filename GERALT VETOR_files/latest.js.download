     */
    function proper_remnant(doc, id) {
        var slot = properPage.getSlotByNumber(id, ['display']);
        if(!slot) {
            properLog.mylog("Couldn't find slot with number: " + id);
            return false;
        }
        log_unfilled(slot);
        return;
    }


