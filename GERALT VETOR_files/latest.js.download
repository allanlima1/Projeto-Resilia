			};
			params = '?gdprIab=' + encodeURIComponent(JSON.stringify(gdprIab))
		}

		userSyncs.add({
			type: 'iframe',
			url: USYNCURL + params,
            bidder: bidder
		});
	});

    return {
		formatRequest: formatRequest,
		send: send,
		bidderInfo: bidderInfo
	};
})();

    bidAdapters.triplelift = (function() {
    var bidderInfo = {
        "rev_share"      : 1,
        "demand_type"    : 'display',
