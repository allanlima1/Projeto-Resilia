                                    var requestId  = resp.bidid || '',
                                        price      = bid.price  || 0.00,
                                        width      = bid.w      || 1,
                                        height     = bid.h      || 1,
                                        creativeId = bid.crid   || '',
                                        currency   = resp.cur   || 'USD',
